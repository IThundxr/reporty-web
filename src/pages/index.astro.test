---
import MainLayout from "../layouts/MainLayout.astro";
---

<MainLayout title="Reporty">
	<div class="flex items-center justify-center w-screen h-screen">
		<div
			class="p-10 space-y-5 bg-white rounded-2xl drop-shadow-lg"
		>
		</div>
	</div>
</MainLayout>

		<div>
			<div class="flex justify-center">
				<RoundedButton iconname="jam:triangle-danger-f" path="/new" service="Report URL" width="25" css="bg-red-500 hover:bg-red-500"/>
				<RoundedButton iconname="jam:triangle-danger-f" path="/new" service="Report User" width="25" css="bg-red-500 hover:bg-red-500"/>
				<RoundedButton iconname="jam:triangle-danger-f" path="/new" service="Report Server" width="25" css="bg-red-500 hover:bg-red-500"/>
			</div>
			<div class="flex justify-center">
				<RoundedButton iconname="jam:triangle-danger-f" path="/new" service="Check URL" width="25" css="bg-green-500 hover:bg-green-500"/>
				<RoundedButton iconname="jam:triangle-danger-f" path="/new" service="Check User" width="25" css="bg-green-500 hover:bg-green-500"/>
				<RoundedButton iconname="jam:triangle-danger-f" path="/new" service="Check Server" width="25" css="bg-green-500 hover:bg-green-500"/>
			</div>
		</div>